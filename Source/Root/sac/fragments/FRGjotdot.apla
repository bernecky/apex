'% SAC Code fragments for outer product loop control.',(⎕UCS 10),'% Robert Bernecky 2005-11-05',(⎕UCS 10 10),'%Fragment  jotdot 000 bidc bidc bidc .',(⎕UCS 10),'inline $ZTYPE $FNAME($XTYPE x, $YTYPE y$SYSVARFGDECL)',(⎕UCS 10),'{ /* SxS outer product */',(⎕UCS 10),'  z = $FN$CT$CT$ZT(to$CT(x),to$CT(y)$SYSVARGKER);',(⎕UCS 10),'  return(z);',(⎕UCS 10),'}',(⎕UCS 10),'%Generate , $FN, $CT$CT$ZT, 000, ., $CT',(⎕UCS 10 10),'%Fragment  jotdot 0** bidc bidc bidc .',(⎕UCS 10),'inline $ZTYPE[*] $FNAME($XTYPE x, $YTYPE[+] y$SYSVARFGDECL)',(⎕UCS 10),'{ /* SxA outer product */',(⎕UCS 10),'  z = $FN$CT$CT$ZT(to$CT(x),to$CT(y)$SYSVARGKER);',(⎕UCS 10),'  return(z);',(⎕UCS 10),'}',(⎕UCS 10),'%Generate , $FN, $CT$CT$ZT, 0**, ., $CT',(⎕UCS 10 10),'%Fragment  jotdot *0* bidc bidc bidc .',(⎕UCS 10),'inline $ZTYPE[*] $FNAME($XTYPE [+] x, $YTYPE y$SYSVARFGDECL)',(⎕UCS 10),'{ /* AxS outer product */',(⎕UCS 10),'  z = $FN$CT$CT$ZT(to$CT(x),to$CT(y)$SYSVARGKER);',(⎕UCS 10),'  return(z);',(⎕UCS 10),'}',(⎕UCS 10),'%Generate , $FN, $CT$CT$ZT, *0*, ., $CT',(⎕UCS 10 10),'%Fragment  jotdot *** bidc bidc bidc .',(⎕UCS 10),'inline $ZTYPE[*] $FNAME($XTYPE [+] x, $YTYPE [+] y$SYSVARFGDECL)',(⎕UCS 10),'{ /* AxA outer product */',(⎕UCS 10),' cell = genarray(shape(y), $OTFILL);',(⎕UCS 10),' z = with {',(⎕UCS 10),'        (. <= iv <= .) {',(⎕UCS 10),'         xitem = to$CT(x[iv]);',(⎕UCS 10),'        } : $FN$CT$CT$ZT(xitem, to$CT(y)$SYSVARGKER);',(⎕UCS 10),'        } : genarray(shape(x), cell);',(⎕UCS 10),' return(z);',(⎕UCS 10),'}',(⎕UCS 10),'%Generate , $FN, $CT$CT$ZT, 0**, ., $CT',(⎕UCS 10)
